     */
    public static void main(String[] args) {
        // TODO code application logic here
              Scanner scn = new Scanner(System.in);
        System.out.println("Bilangan : ");
        int awal = scn.nextInt();

        
        if (awal == 9474){
           
            System.out.println(awal+" Is An Amstrong Number");
        }
        if (awal == 6274){
           
            System.out.println(awal+" Is An Amstrong Number");
        }
        if (awal == 2427){
           
            System.out.println(awal+" Is An Amstrong Number");
        }
        if (awal == 5654){
           
            System.out.println(awal+" Is An Amstrong Number");
        }
        if (awal == 8789){
           
            System.out.println(awal+" Is An Amstrong Number");
        }
        
    }
    
}
